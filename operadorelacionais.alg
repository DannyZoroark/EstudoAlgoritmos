algoritmo "OPeradores parte 2"

var

   A, B , C: Inteiro

inicio
      Escreval("Operadores Lógicos")
      A <- 2
      B <- 3
      C <- 5
      Escreval( Verdadeiro e Falso )
      Escreval( (A=B) ou (C>A))//quando operador for "ou" as duas não precisam ser verdaeiras
      Escreval( nao((A=B) ou (C>A)))//não verdadeiro = falso

fimalgoritmo
